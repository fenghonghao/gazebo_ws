<launch>
  <rosparam command="load" file="$(find simlaunch)/config/racecar.yaml"/>
  
  <!-- 启动赛车Gazebo仿真环境 -->
  <include file="$(find racecar_description)/launch/racecar_gazebo.launch"/>
  
  <!-- 启动建图和导航功能 -->
  <include file="$(find simlaunch)/launch/map.launch"/>
  
  <!-- 启动赛车控制器 -->
   <node name="controller" pkg="controller" type="pure_pursuit_bicycle" output="screen"/>
   
</launch>